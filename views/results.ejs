<div class="jumbotron text-center">
  <h1 class="text-info">Predicted Food Details</h1>
  <table class="table table-striped table-hover table-borderless" style="max-width: 400px; margin: 0 auto;">
    <thead>
      <tr class="table-primary">
        <th scope="col">Food</th>
        <th scope="col">Calorie</th>
      </tr>
    </thead>
    <tbody>
      <% for (var i = 0; i < result.length ; i++) { %>
        <tr>
          <td class="text-success"><%- result[i].food %></td>
          <td class="text-warning"><%- result[i].calorie %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</div>